<aura:tokens extends="force:base"> 
    <!-- Remap values from the branding panel -->
    <aura:token name="colorTextDefaultSW"					value="{! colorTextDefault }" />					<!-- Text Color -->
    <aura:token name="colorTextLabelSW"						value="{! colorTextLabel }" />						<!-- Detail Text Color -->
    <aura:token name="colorBrandSW"							value="{! colorBrand }" />							<!-- Action Color -->
    <aura:token name="colorBackgroundButtonBrandActiveSW"	value="{! colorBackgroundButtonBrandActive }" />	<!-- Action Color Hover -->
    <aura:token name="colorTextLinkSW"						value="{! colorTextLink }" />						<!-- Link Color -->
    <aura:token name="colorBackgroundButtonDefaultHoverSW"	value="{! colorBackgroundButtonDefaultHover }" />	<!-- Link Color Hover -->
    <aura:token name="colorTextLinkActiveSW"				value="{! colorTextLinkActive }" />					<!-- Link Color Darker -->
    <aura:token name="colorTextButtonBrandSW"				value="{! colorTextButtonBrand }" /> 				<!-- Overlay Text Color -->
    <aura:token name="colorBorderSW"						value="{! colorBorder }" />							<!-- Border Color -->
    <aura:token name="textTransformSW"						value="{! textTransform }" />						<!-- Text Case -->
    
    <aura:token name="colorZebraStripeSW"					value="#f5f5f5" /> 
    <aura:token name="sizeBorderSW"							value="1px" />
    <aura:token name="styleBorderSW"						value="solid" />
    <aura:token name="styleBorderRadiusSW"					value="15px" />
    <aura:token name="styleBorderRadiusDashboard"			value="5px" />
    <aura:token name="colorAlertNew"						value="#ffeb3b" /> 
    
    <!-- 	
		DONORPERFECT COLORS
        Use these custom color names in LCs 
	-->
    <aura:token name="dpColor1"			value="{! dpBlue }" />
