    <aura:token name="dpColor3"			value="{! dpLtBlue }" />
    <aura:token name="dpColor4"			value="{! dpRed }" />
    <aura:token name="dpColor5"			value="{! dpOrange }" />
    <aura:token name="dpTextSecondary"	value="#727188" />
    
    <!-- Element styles for DP -->
    <aura:token name="dpBorderColor"	value="#dfe4ec" />
    <aura:token name="dpBorderWidth"	value="1px" />
    <aura:token name="dpBorderStyle"	value="solid" />
    <aura:token name="dpBoxShadow"		value="0 1px 5px #dfe4ec" /> 
	<aura:token name="ezTextSecondary"	value="#99a9bf" />
	
